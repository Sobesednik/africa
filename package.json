{
  "name": "africa",
  "version": "1.2.0",
  "description": "A library to interactively create and read configuration files.",
  "main": "compile/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec",
    "test-build": "ALAMODE_ENV=test-compile yarn test",
    "compile": "depack-dev src/africa.js -o compile/africa.js -a -c -p --source_map_include_content -O 2018",
    "lint": "eslint .",
    "example/": "node example/index.js example/example.js",
    "example/local.js": "node example/index.js example/local.js",
    "doc": "doc documentary -o README.md",
    "e": "alanode"
  },
  "files": [
    "compile",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/africa.git"
  },
  "keywords": [
    "africa",
    "rc",
    "configuration",
    "persistent",
    "dot"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/africa/issues"
  },
  "homepage": "https://github.com/artdecocode/africa#readme",
  "devDependencies": {
    "alamode": "^2.3.6",
    "bosom": "^2.0.0",
    "documentary": "^1.29.0",
    "eslint-config-artdeco": "^1.0.1",
    "reloquent": "^1.3.1",
    "spawncommand": "^2.2.0",
    "typal": "^1.19.1",
    "wrote": "^1.4.0",
    "yarn-s": "^1.1.0",
    "zoroaster": "^4.1.1-alpha"
  },
  "srcDependencies": {
    "bosom": "^2.0.0",
    "reloquent": "^1.3.1"
  }
}
